<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Sunburst Theme">
    <object name="wcc_theme"
        meta_type="Filesystem Directory View"
        directory="wcc.theme:skins/wcc_theme"/>
    <skin-path name="*">
        <layer name="wcc_theme"
                insert-after="custom"/>
    </skin-path>
</object>

